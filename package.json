{
  "name": "reservapp-web",
  "version": "1.0.0",
  "description": "ReservApp - Modern reservation management system",
  "type": "module",
  "private": true,
  "sideEffects": [
    "**/*.css",
    "**/*.styled.ts",
    "**/*.styled.tsx",
    "src/libs/ui/styles/GlobalStyles.tsx",
    "src/libs/ui/providers/**/*.tsx"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "yarn lint:tsx && yarn lint:css && yarn type-check",
    "lint:tsx": "eslint . --config eslint.config.js --cache",
    "lint:css": "stylelint \"**/*.styled.{ts,tsx}\" --cache",
    "lint:fix": "yarn lint:tsx --fix; yarn lint:css --fix; yarn format",
    "lint:fix:safe": "yarn lint:tsx --fix --max-warnings 0 && yarn lint:css --fix --max-warnings 0 && yarn format",
    "lint:fix:all": "yarn lint:fix && yarn type-check",
    "lint:errors": "yarn lint:tsx --quiet && yarn lint:css --quiet && yarn type-check",
    "lint:errors:tsx": "eslint . --config eslint.config.js --cache --quiet",
    "lint:errors:css": "stylelint \"**/*.styled.{ts,tsx}\" --cache --quiet",
    "lint:staged": "eslint --cache --fix",
    "lint:staged:css": "stylelint --cache --fix",
    "lint:performance": "TIMING=1 yarn lint:tsx",
    "lint:hooks": "eslint 'src/libs/presentation/hooks/**/*.{ts,tsx}' --cache --fix",
    "lint:components": "eslint 'src/libs/ui/components/**/*.{ts,tsx}' --cache --fix",
    "lint:services": "eslint 'src/libs/services/**/*.{ts,tsx}' --cache --fix",
    "lint:api": "eslint 'src/app/api/**/*.ts' --cache --fix",
    "lint:modules": "eslint 'src/modules/**/*.{ts,tsx}' --cache --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,md,json}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,md,json}\" --ignore-path .gitignore",
    "analyze:exports": "grep -r 'export default' src/ --include='*.ts' --include='*.tsx' | wc -l",
    "analyze:console": "grep -r 'console\\.' src/ --include='*.ts' --include='*.tsx' | wc -l",
    "analyze:unused": "yarn lint:tsx --rule 'import/no-unused-modules: error'",
    "clean:cache": "rm -rf .next .build-cache node_modules/.cache",
    "clean:build": "rm -rf .next out dist build",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --config jest.config.cjs --watch",
    "test:coverage": "jest --config jest.config.cjs --coverage",
    "test:ci": "jest --config jest.config.cjs --ci --coverage --watchAll=false --passWithNoTests",
    "test:integration": "jest --config jest.config.cjs --testPathPattern=integration",
    "test:performance": "jest --config jest.config.cjs --testPathPattern=performance",
    "type-check": "tsc --noEmit --incremental",
    "pre-commit": "yarn lint:errors && yarn format:check",
    "pre-push": "yarn lint && yarn test --passWithNoTests",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "vercel:deploy": "vercel --prod",
    "vercel:preview": "vercel",
    "vercel:inspect": "vercel inspect",
    "vercel:env": "vercel env",
    "vercel:logs": "vercel logs",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force && yarn db:seed",
    "docs:swagger": "node scripts/generate-swagger-docs.js",
    "docs:check": "yarn docs:swagger",
    "generate:module": "node scripts/generate-module.js",
    "generate:component": "node scripts/generate-component.js",
    "generate:hook": "node scripts/generate-hook.js"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "@googlemaps/js-api-loader": "^1.16.10",
    "@hookform/resolvers": "^3.3.2",
    "@prisma/client": "6.15.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@stripe/react-stripe-js": "^3.9.0",
    "@stripe/stripe-js": "^7.8.0",
    "@types/google.maps": "^3.58.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.6.2",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "date-fns": "^3.6.0",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.294.0",
    "mysql2": "^3.14.3",
    "next": "^15.1.4",
    "prisma": "6.15.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.48.2",
    "react-is": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.8.0",
    "recharts": "^3.1.2",
    "redux-persist": "^6.0.0",
    "resend": "^4.8.0",
    "stripe": "^18.4.0",
    "styled-components": "^6.1.14",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-dist": "^5.27.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-react": "^7.14.5",
    "@storybook/addon-essentials": "^8.4.6",
    "@storybook/addon-interactions": "^8.4.6",
    "@storybook/react-vite": "^8.4.6",
    "@storybook/testing-library": "^0.2.2",
    "@swc/core": "^1.5.7",
    "@swc/helpers": "^0.5.11",
    "@swc/jest": "^0.2.39",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/jest": "^29.5.14",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/swagger-ui-dist": "^3.30.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^9.8.0",
    "eslint-config-next": "^15.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.1",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-testing-library": "^7.6.3",
    "eslint-restricted-globals": "^0.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-styled-components": "^7.2.0",
    "jest-watch-typeahead": "^3.0.1",
    "postcss-styled-syntax": "^0.7.1",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "storybook": "^8.4.6",
    "stylelint": "^16.21.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-order": "^7.0.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.20.3",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  }
}
